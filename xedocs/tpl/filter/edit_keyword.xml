<filter name="edit_keyword" module="xedocs" act="procXedocsAdminEditKeyword" >

    <form>
        <node target="module_srl" required="true" />
        <node target="keyword" required="true" />
        <node target="title" required="true" />
        <node target="orig_title" required="false" />
        <node target="target_document_srl" required="true" />
    </form>

    <response callback_func="completeEditKeyword">
        <tag name="error" />
        <tag name="message" />
        <tag name="page" />
    </response>

</filter>
