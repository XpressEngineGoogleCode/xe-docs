<filter name="compile_keywords" module="xedocs" act="procXedocsAdminCompileKeywords" >

    <form>
        <node target="module_srl" required="true" />
        <node target="help_name" required="true" />
        <node target="filter_keyword" required = "false" />
    </form>

    <response callback_func="completeCompileKeywords">
        <tag name="error" />
        <tag name="message" />
        <tag name="page" />
    </response>

</filter>
