<query id="getAliasesByTitle" action="select">
    <tables>
        <table name="document_aliases" alias="a" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="a.alias_title"  var="alias_title"  notnull="notnull" pipe="and" />
    </conditions>
</query>
