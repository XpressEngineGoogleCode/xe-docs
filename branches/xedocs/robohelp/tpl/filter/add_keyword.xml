<filter name="add_keyword" module="xedocs" act="procXedocsAdminAddKeyword" >

    <form>
        <node target="module_srl" required="true" />
        <node target="keyword" required="true" />
        <node target="title" required="true" />
        <node target="target_document_srl" required="true" />
    </form>

    <response callback_func="completeAddKeyword">
        <tag name="error" />
        <tag name="message" />
        <tag name="page" />
    </response>

</filter>
